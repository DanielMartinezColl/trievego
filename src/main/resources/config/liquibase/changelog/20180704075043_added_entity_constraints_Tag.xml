<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Tag.
    -->
    <changeSet id="20180704075043-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="estado_id"
                                 baseTableName="tag"
                                 constraintName="fk_tag_estado_id"
                                 referencedColumnNames="id"
                                 referencedTableName="estado"/>

        <addForeignKeyConstraint baseColumnNames="tags_id"
                                 baseTableName="tag_user"
                                 constraintName="fk_tag_user_tags_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tag"/>
        <addForeignKeyConstraint baseColumnNames="users_id"
                                 baseTableName="tag_user"
                                 constraintName="fk_tag_user_users_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
        
    </changeSet>
</databaseChangeLog>
